{
  "name": "kusikusicms/admin",
  "description": "Admin UI for managing content in KusikusiCMS.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^12.0",
    "illuminate/view": "^12.0",
    "kusikusicms/models": "^0.1 || @dev"
  },
  "autoload": {
    "psr-4": {
      "KusikusiCMS\\Admin\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KusikusiCMS\\Admin\\AdminServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
